<UserControl x:Class="M3.Cord.Pages.ProductionRecordPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:ctrl="clr-namespace:NLib.Wpf.Controls;assembly=NLib.Wpf.Controls"
             xmlns:page="clr-namespace:NLib.Wpf.Pages;assembly=NLib.Wpf.Controls"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d"
             d:DesignHeight="1000"
             d:DesignWidth="1730"
             Loaded="UserControl_Loaded"
             Unloaded="UserControl_Unloaded">
    <UserControl.Resources>
        <!--  LOCAL STYLES  -->
        <Style TargetType="Border">
            <Setter Property="Padding" Value="5" />
            <Setter Property="BorderBrush" Value="DimGray" />
            <Setter Property="BorderThickness" Value="0.5" />
        </Style>
        <Style TargetType="TextBlock" >
            <Setter Property="FontSize" Value="18" />
            <Setter Property="FontFamily" Value="Microsoft Sans Serif"/>
            <!--<Setter Property="FontFamily" Value="Comic Sans MS"/>-->
            <Setter Property="FontWeight" Value="Normal"/>
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="HorizontalAlignment" Value="Center" />
        </Style>
        <Style TargetType="TextBox" BasedOn="{StaticResource CommonTextBox}">
            <Setter Property="FontSize" Value="18" />
            <Setter Property="FontFamily" Value="Microsoft Sans Serif"/>
            <Setter Property="FontWeight" Value="Normal"/>
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="HorizontalAlignment" Value="Center" />
        </Style>
        <Style TargetType="CheckBox" BasedOn="{StaticResource CommonCheckBox}">
            <Setter Property="FontSize" Value="18" />
            <Setter Property="FontFamily" Value="Microsoft Sans Serif"/>
            <Setter Property="FontWeight" Value="Normal"/>
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="HorizontalAlignment" Value="Center" />
        </Style>
    </UserControl.Resources>
    <page:NPage HeaderText="ใบบันทึกค่าสภาวะการผลิต S-8-1">
        <DockPanel LastChildFill="True" Margin="5">
            <!--  BUTTONS  -->
            <DockPanel LastChildFill="True"
                       DockPanel.Dock="Bottom"
                       Height="40">
                <StackPanel Orientation="Horizontal"
                            DockPanel.Dock="Right"
                            VerticalAlignment="Center">
                    <ctrl:FontAwesomeButton x:Name="cmdSave"
                                            Click="cmdSave_Click"
                                            DockPanel.Dock="Left"
                                            Margin="5,0,0,0"
                                            IconType="Save"
                                            Text="Save" />
                    <!--  HOME  -->
                    <ctrl:FontAwesomeButton x:Name="cmdHome"
                                            Margin="5,0,0,0"
                                            IconType="Home"
                                            Text="Back"
                                            Click="cmdHome_Click" />
                </StackPanel>
                <Grid />
            </DockPanel>
            <!--  TOP ENTRY  -->
            <DockPanel LastChildFill="True" DockPanel.Dock="Top">
                <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                    <DockPanel Margin="0,0,5,5"
                               DockPanel.Dock="Top"
                               LastChildFill="True"
                               VerticalAlignment="Top"
                               HorizontalAlignment="Stretch">
                        <StackPanel Orientation="Horizontal" DockPanel.Dock="Top">
                            <ctrl:NTextBox x:Name="txtLotNo"
                                           CaptionText="Lot No"
                                           Width="200" IsEnabled="False"/>
                            <!--  SOLUTION LOT NO  -->
                            <ctrl:NComboBox x:Name="cbProducts"
                                    DockPanel.Dock="Left"
                                    CaptionText="Product :"
                                    DisplayMemberPath="ProductCode"
                                    Width="300"
                                    SelectionChanged="cbProducts_SelectionChanged" />
                            <!--  SEARCH  -->
                            <Button x:Name="cmdSearch"
                                    Content="Search"
                                    Width="150"
                                    VerticalAlignment="Bottom"
                                    Margin="5,2"
                                    Style="{StaticResource InlineButton}"
                                    Click="cmdSearch_Click" />

                            <!--  CLEAR  -->
                            <Button x:Name="cmdClear"
                                    Content="Clear"
                                    DockPanel.Dock="Left"
                                    Width="150"
                                    VerticalAlignment="Bottom"
                                    Margin="5,2"
                                    Style="{StaticResource InlineButton}"
                                    Click="cmdClear_Click" />

                            <Button x:Name="cmdAddDetail"
                                    Content="Add Detail"
                                    DockPanel.Dock="Left"
                                    Width="150"
                                    VerticalAlignment="Bottom"
                                    Margin="5,2"
                                    Style="{StaticResource InlineButton}"
                                    Click="cmdAddDetail_Click" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" DockPanel.Dock="Top">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>

                                <!--#region COLUMN HEADERS-->

                                <Border Grid.Row="0"
                                        Grid.Column="0"
                                        Grid.ColumnSpan="2">
                                    <ctrl:NDatePicker x:Name="dtRecordDate"
                                                      CaptionText="Date (ว.ด.ป)"
                                                      SelectedDate="{Binding RecordDate, StringFormat='{}{0:dd/MM/yy}'}" />
                                </Border>
                                <Border Grid.Row="1"
                                        Grid.Column="0"
                                        Grid.ColumnSpan="2">
                                    <TextBlock Text="Process (ขบวนการผลิต)" TextAlignment="Center" />
                                </Border>
                                <Border Grid.Row="3" Grid.Column="0">
                                    <StackPanel Orientation="Vertical"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center">
                                        <TextBlock Text="HS (ความร้อน)" TextAlignment="Center" />
                                        <CheckBox Content="" IsChecked="{Binding ProcessHS}" />
                                    </StackPanel>
                                </Border>
                                <Border Grid.Row="3" Grid.Column="1">
                                    <StackPanel Orientation="Vertical"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center">
                                        <TextBlock Text="DIP (การจุ่ม)" TextAlignment="Center" />
                                        <CheckBox Content="" IsChecked="{Binding ProcessDIP}" />
                                    </StackPanel>
                                </Border>

                                <Border Grid.Row="0" Grid.Column="3">
                                    <StackPanel Orientation="Vertical"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center">
                                        <TextBlock Text="USER (ลูกค้า)" TextAlignment="Center" />
                                        <TextBox Width="200" Text="{Binding Customer}" />
                                    </StackPanel>
                                </Border>
                                <Border Grid.Row="1" Grid.Column="3">
                                    <TextBlock Text="Counter M (ความยาวเมตร)" TextAlignment="Center" />
                                </Border>
                                <Border Grid.Row="2" Grid.Column="3">
                                    <StackPanel Orientation="Horizontal"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center">
                                        <StackPanel Orientation="Vertical">
                                            <TextBox Width="70" Text="{Binding Counter, StringFormat='{}{0:#,##0.##}'}" />
                                        </StackPanel>
                                        <StackPanel Orientation="Vertical">
                                            <TextBlock Text=" ± " FontSize="18" />
                                        </StackPanel>
                                        <StackPanel Orientation="Vertical">
                                            <TextBox Width="70" Text="{Binding CounterErr, StringFormat='{}{0:#,##0.##}'}" />
                                        </StackPanel>
                                    </StackPanel>
                                </Border>

                                <Border Grid.Row="0"
                                        Grid.Column="4"
                                        Grid.ColumnSpan="2">
                                    <StackPanel Orientation="Vertical"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center">
                                        <TextBlock Text="Cord Structure (โครงสร้างเชือก)" TextAlignment="Center"/>
                                        <TextBox Width="200" Text="{Binding CordStructure}" />
                                    </StackPanel>
                                </Border>
                                <Border Grid.Row="1" Grid.Column="4">
                                    <TextBlock Text="Item Code" TextAlignment="Center" />
                                </Border>
                                <Border Grid.Row="2" Grid.Column="4">
                                    <TextBox Width="100" Text="{Binding ProductCode}" IsEnabled="False" />
                                </Border>
                                <Border Grid.Row="1" Grid.Column="5">
                                    <TextBlock Text="Lot No" TextAlignment="Center" />
                                </Border>
                                <Border Grid.Row="2" Grid.Column="5">
                                    <TextBox Width="100" Text="{Binding LotNo}" IsEnabled="False"/>
                                </Border>

                                <Border Grid.Row="0"
                                        Grid.Column="6"
                                        Grid.ColumnSpan="2">
                                    <StackPanel Orientation="Vertical"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center">
                                        <StackPanel Orientation="Vertical" HorizontalAlignment="Center">
                                            <TextBlock Text="DIP Bath No.1" FontSize="18" />
                                            <TextBlock Text="Solution Name" FontSize="18" />
                                        </StackPanel>

                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <TextBox Width="200" Text="{Binding Bath1SolutionName}" />
                                        </StackPanel>
                                    </StackPanel>
                                </Border>
                                <Border Grid.Row="1"
                                        Grid.Column="6"
                                        Grid.ColumnSpan="2">
                                    <TextBlock Text="Nip Prcssure (kg/cm2)" TextAlignment="Center" />
                                </Border>
                                <Border Grid.Row="2" Grid.Column="6">
                                    <StackPanel Orientation="Vertical"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center">
                                        <StackPanel Orientation="Vertical" HorizontalAlignment="Center">
                                            <TextBlock Text="Front" FontSize="18"/>
                                            <TextBox Width="70" Text="{Binding Bath1NipFront}" />
                                        </StackPanel>
                                    </StackPanel>
                                </Border>
                                <Border Grid.Row="2" Grid.Column="7">
                                    <StackPanel Orientation="Vertical"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center">
                                        <StackPanel Orientation="Vertical" HorizontalAlignment="Center">
                                            <TextBlock Text="Back" FontSize="18"/>
                                            <StackPanel Orientation="Horizontal"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center">
                                                <StackPanel Orientation="Vertical">
                                                    <TextBox Width="70" Text="{Binding Bath1NipBack, StringFormat='{}{0:#,##0.##}'}" />
                                                </StackPanel>
                                                <StackPanel Orientation="Vertical">
                                                    <TextBlock Text=" ± " FontSize="18"/>
                                                </StackPanel>
                                                <StackPanel Orientation="Vertical">
                                                    <TextBox Width="70" Text="{Binding Bath1NipBackErr, StringFormat='{}{0:#,##0.##}'}" />
                                                </StackPanel>
                                            </StackPanel>
                                        </StackPanel>
                                    </StackPanel>
                                </Border>

                                <Border Grid.Row="0"
                                        Grid.Column="8"
                                        Grid.ColumnSpan="2">
                                    <StackPanel Orientation="Vertical"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center">
                                        <StackPanel Orientation="Vertical" HorizontalAlignment="Center">
                                            <TextBlock Text="DIP Bath No.2" FontSize="18"/>
                                            <TextBlock Text="Solution Name" FontSize="18"/>
                                        </StackPanel>

                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <TextBox Width="200" Text="{Binding Bath2SolutionName}" />
                                        </StackPanel>
                                    </StackPanel>
                                </Border>
                                <Border Grid.Row="1"
                                        Grid.Column="8"
                                        Grid.ColumnSpan="2">
                                    <TextBlock Text="Nip Prcssure (kg/cm2)" TextAlignment="Center" />
                                </Border>
                                <Border Grid.Row="2" Grid.Column="8">
                                    <StackPanel Orientation="Vertical"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center">
                                        <StackPanel Orientation="Vertical" HorizontalAlignment="Center">
                                            <TextBlock Text="Front" FontSize="18"/>
                                            <TextBox Width="70" Text="{Binding Bath2NipFront}" />
                                        </StackPanel>
                                    </StackPanel>
                                </Border>
                                <Border Grid.Row="2" Grid.Column="9">
                                    <StackPanel Orientation="Vertical"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center">
                                        <StackPanel Orientation="Vertical" HorizontalAlignment="Center">
                                            <TextBlock Text="Back" FontSize="18"/>
                                            <StackPanel Orientation="Horizontal"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center">
                                                <StackPanel Orientation="Vertical">
                                                    <TextBox Width="70" Text="{Binding Bath2NipBack, StringFormat='{}{0:#,##0.##}'}" />
                                                </StackPanel>
                                                <StackPanel Orientation="Vertical">
                                                    <TextBlock Text=" ± " FontSize="18"/>
                                                </StackPanel>
                                                <StackPanel Orientation="Vertical">
                                                    <TextBox Width="70" Text="{Binding Bath2NipBackErr, StringFormat='{}{0:#,##0.##}'}" />
                                                </StackPanel>
                                            </StackPanel>
                                        </StackPanel>
                                    </StackPanel>
                                </Border>

                                <Border Grid.Row="0"
                                        Grid.Column="10"
                                        Grid.ColumnSpan="2">
                                    <StackPanel Orientation="Vertical"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center">
                                        <TextBlock Text="SOFNER (ปรับให้นิ่ม)" TextAlignment="Center" />

                                        <StackPanel Orientation="Horizontal"
                                                    HorizontalAlignment="Center"
                                                    VerticalAlignment="Center">

                                            <StackPanel Orientation="Vertical"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center">
                                                <TextBlock Text="Sofner"
                                                           Width="80"
                                                           TextAlignment="Center" />
                                                <CheckBox Content="" IsChecked="{Binding Sofner}" />
                                            </StackPanel>
                                            <StackPanel Orientation="Vertical"
                                                        HorizontalAlignment="Center"
                                                        VerticalAlignment="Center">
                                                <TextBlock Text="Darw Nip"
                                                           Width="80"
                                                           TextAlignment="Center" />
                                                <CheckBox Content="" IsChecked="{Binding DarwNip}" />
                                            </StackPanel>
                                        </StackPanel>
                                    </StackPanel>
                                </Border>
                                <Border Grid.Row="1"
                                        Grid.Column="10"
                                        Grid.ColumnSpan="2">
                                    <StackPanel Orientation="Vertical"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center">
                                        <TextBlock Text="Paper tube color use" TextAlignment="Center" />
                                        <TextBlock Text="การใช้หลอดกระดาษสี" TextAlignment="Center" />
                                    </StackPanel>
                                </Border>
                                <Border Grid.Row="2"
                                        Grid.Column="10"
                                        Grid.ColumnSpan="2">
                                    <TextBox Width="150" Text="{Binding PaperTubeColorUse}" />
                                </Border>


                                <Border Grid.Row="0"
                                        Grid.Column="12"
                                        Grid.ColumnSpan="2"
                                        Grid.RowSpan="3">
                                    <StackPanel Orientation="Vertical"
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center">
                                        <TextBlock Text="TENSION (g/cord) (แรงดึง)" TextAlignment="Center" FontSize="18"/>
                                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="Auto" />
                                                    <ColumnDefinition Width="Auto" />
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto" />
                                                    <RowDefinition Height="Auto" />
                                                    <RowDefinition Height="Auto" />
                                                    <RowDefinition Height="Auto" />
                                                    <RowDefinition Height="Auto" />
                                                </Grid.RowDefinitions>
                                                <Border Grid.Row="0" Grid.Column="0">
                                                    <TextBlock Text="Zone" FontSize="18"/>
                                                </Border>
                                                <Border Grid.Row="0" Grid.Column="1">
                                                    <TextBlock Text="Monitor display (kg)" FontSize="18"/>
                                                </Border>

                                                <Border Grid.Row="1" Grid.Column="0">
                                                    <TextBlock Text="D" FontSize="18"/>
                                                </Border>
                                                <Border Grid.Row="1" Grid.Column="1">
                                                    <TextBox Width="70" Text="{Binding TensionD, StringFormat='{}{0:#,##0.##}'}" />
                                                </Border>

                                                <Border Grid.Row="2" Grid.Column="0">
                                                    <TextBlock Text="H" FontSize="18"/>
                                                </Border>
                                                <Border Grid.Row="2" Grid.Column="1">
                                                    <TextBox Width="70" Text="{Binding TensionH, StringFormat='{}{0:#,##0.##}'}" />
                                                </Border>

                                                <Border Grid.Row="3" Grid.Column="0">
                                                    <TextBlock Text="N" FontSize="18"/>
                                                </Border>
                                                <Border Grid.Row="3" Grid.Column="1">
                                                    <TextBox Width="70" Text="{Binding TensionN, StringFormat='{}{0:#,##0.##}'}" />
                                                </Border>

                                                <Border Grid.Row="4" Grid.Column="0">
                                                    <TextBlock Text="Winder" FontSize="18"/>
                                                </Border>
                                                <Border Grid.Row="4" Grid.Column="1">
                                                    <TextBox Width="70" Text="{Binding TensionWinder, StringFormat='{}{0:#,##0.##}'}" />
                                                </Border>
                                            </Grid>


                                        </StackPanel>
                                    </StackPanel>


                                </Border>


                                <!--#endregion-->

                            </Grid>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" DockPanel.Dock="Top">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>

                                <!--#region COLUMN HEADERS-->

                                <Border Grid.Row="0"
                                        Grid.Column="0"
                                        Grid.ColumnSpan="3"
                                        Background="Silver">
                                    <TextBlock Text="Stretch (%)" HorizontalAlignment="Center" FontSize="18" />
                                </Border>
                                <Border Grid.Row="1"
                                        Grid.Column="0"
                                        Background="Silver">
                                    <TextBlock Text="D" HorizontalAlignment="Center" FontSize="18" />
                                </Border>
                                <Border Grid.Row="2"
                                        Grid.Column="0"
                                        Background="Silver">
                                    <TextBlock Text="ช่วงแห้ง" HorizontalAlignment="Center" FontSize="18" />
                                </Border>
                                <Border Grid.Row="1"
                                        Grid.Column="1"
                                        Background="Silver">
                                    <TextBlock Text="H" HorizontalAlignment="Center" FontSize="18" />
                                </Border>
                                <Border Grid.Row="2"
                                        Grid.Column="1"
                                        Background="Silver">
                                    <TextBlock Text="ช่วงร้อน" HorizontalAlignment="Center" FontSize="18" />
                                </Border>
                                <Border Grid.Row="1"
                                        Grid.Column="2"
                                        Background="Silver">
                                    <TextBlock Text="N" HorizontalAlignment="Center" FontSize="18" />
                                </Border>
                                <Border Grid.Row="2"
                                        Grid.Column="2"
                                        Background="Silver">
                                    <TextBlock Text="ช่วงปกติ" HorizontalAlignment="Center" FontSize="18" />
                                </Border>

                                <Border Grid.Row="0"
                                        Grid.Column="3"
                                        Grid.ColumnSpan="2"
                                        Background="Silver">
                                    <TextBlock Text="Temp (C) (อุณหภูมิ)" HorizontalAlignment="Center" FontSize="18" />
                                </Border>
                                <Border Grid.Row="1"
                                        Grid.Column="3"
                                        Background="Silver">
                                    <TextBlock Text="D" HorizontalAlignment="Center" FontSize="18" />
                                </Border>
                                <Border Grid.Row="2"
                                        Grid.Column="3"
                                        Background="Silver">
                                    <TextBlock Text="ช่วงแห้ง" HorizontalAlignment="Center" FontSize="18" />
                                </Border>
                                <Border Grid.Row="1"
                                        Grid.Column="4"
                                        Background="Silver">
                                    <TextBlock Text="HN" HorizontalAlignment="Center" FontSize="18" />
                                </Border>
                                <Border Grid.Row="2"
                                        Grid.Column="4"
                                        Background="Silver">
                                    <TextBlock Text="ช่วงร้อน/แห้ง" HorizontalAlignment="Center" FontSize="18" />
                                </Border>

                                <Border Grid.Row="0"
                                        Grid.Column="5"
                                        Background="Silver">
                                    <TextBlock Text="Speed" HorizontalAlignment="Center" FontSize="18" />
                                </Border>
                                <Border Grid.Row="1"
                                        Grid.Column="5"
                                        Background="Silver">
                                    <TextBlock Text="(m/min)" HorizontalAlignment="Center" FontSize="18" />
                                </Border>
                                <Border Grid.Row="2"
                                        Grid.Column="5"
                                        Background="Silver">
                                    <TextBlock Text="ความเร็ว" HorizontalAlignment="Center" FontSize="18" />
                                </Border>

                                <Border Grid.Row="0"
                                        Grid.Column="6"
                                        Background="Silver">
                                    <TextBlock Text="Treat" HorizontalAlignment="Center" FontSize="18" />
                                </Border>
                                <Border Grid.Row="1"
                                        Grid.Column="6"
                                        Background="Silver">
                                    <TextBlock Text="Cord" HorizontalAlignment="Center" FontSize="18" />
                                </Border>
                                <Border Grid.Row="2"
                                        Grid.Column="6"
                                        Background="Silver">
                                    <TextBlock Text="จำนวนเส้น" HorizontalAlignment="Center" FontSize="18" />
                                </Border>

                                <Border Grid.Row="0"
                                        Grid.Column="7"
                                        Background="Silver">
                                    <TextBlock Text="Doffing" HorizontalAlignment="Center" FontSize="18" />
                                </Border>
                                <Border Grid.Row="1"
                                        Grid.Column="7"
                                        Background="Silver">
                                    <TextBlock Text="Length (M)" HorizontalAlignment="Center" FontSize="18" />
                                </Border>
                                <Border Grid.Row="2"
                                        Grid.Column="7"
                                        Background="Silver">
                                    <TextBlock Text="ความยาว" HorizontalAlignment="Center" FontSize="18" />
                                </Border>

                                <Border Grid.Row="0"
                                        Grid.Column="8"
                                        Background="Silver">
                                    <TextBlock Text="Weight" HorizontalAlignment="Center" FontSize="18" />
                                </Border>
                                <Border Grid.Row="1"
                                        Grid.Column="8"
                                        Background="Silver">
                                    <TextBlock Text="(kg)" HorizontalAlignment="Center" FontSize="18" />
                                </Border>
                                <Border Grid.Row="2"
                                        Grid.Column="8"
                                        Background="Silver">
                                    <TextBlock Text="น้ำหนัก" HorizontalAlignment="Center" FontSize="18" />
                                </Border>

                                <Border Grid.Row="0"
                                        Grid.Column="9"
                                        Background="Silver">
                                    <TextBlock Text="Spindle" HorizontalAlignment="Center" FontSize="18" />
                                </Border>
                                <Border Grid.Row="1"
                                        Grid.Column="9"
                                        Background="Silver">
                                    <TextBlock Text="Use" HorizontalAlignment="Center" FontSize="18" />
                                </Border>
                                <Border Grid.Row="2"
                                        Grid.Column="9"
                                        Background="Silver">
                                    <TextBlock Text="การใช้แกน" HorizontalAlignment="Center" FontSize="18" />
                                </Border>

                                <Border Grid.Row="0"
                                        Grid.Column="10"
                                        Grid.ColumnSpan="2"
                                        Background="Silver">
                                    <TextBlock Text="Production (ผลิตภัณฑ์)" HorizontalAlignment="Center" FontSize="18" />
                                </Border>
                                <Border Grid.Row="1"
                                        Grid.Column="10"
                                        Background="Silver">
                                    <TextBlock Text="Good" HorizontalAlignment="Center" FontSize="18" />
                                </Border>
                                <Border Grid.Row="2"
                                        Grid.Column="10"
                                        Background="Silver">
                                    <TextBlock Text="ผลิตภัณฑ์ดี" HorizontalAlignment="Center" FontSize="18" />
                                </Border>
                                <Border Grid.Row="1"
                                        Grid.Column="11"
                                        Background="Silver">
                                    <TextBlock Text="Total" HorizontalAlignment="Center" FontSize="18" />
                                </Border>
                                <Border Grid.Row="2"
                                        Grid.Column="11"
                                        Background="Silver">
                                    <TextBlock Text="ผลรวมน้ำหนัก" HorizontalAlignment="Center" FontSize="18" />
                                </Border>

                                <!--#endregion-->

                                <!--#region SC-->

                                <Border Grid.Row="3" Grid.Column="2">
                                    <!--<CheckBox Content="" IsChecked="{Binding S8CoolingWaterSystemBath1SC}" />-->
                                    <TextBlock VerticalAlignment="Center" HorizontalAlignment="Center">
                                        <TextBlock.Style>
                                            <Style TargetType="{x:Type TextBlock}">
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding S8CoolingWaterSystemBath1SC}" Value="True">
                                                        <Setter Property="Text" Value="SC" />
                                                        <Setter Property="FontWeight" Value="DemiBold" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </TextBlock.Style>
                                    </TextBlock>
                                </Border>
                                <Border Grid.Row="3" Grid.Column="3">
                                    <!--<CheckBox Content="" IsChecked="{Binding S8CoolingWaterSystemBath2SC}" />-->
                                    <TextBlock VerticalAlignment="Center" HorizontalAlignment="Center">
                                        <TextBlock.Style>
