                    <TextBlock Text=" ± " />
                    <TextBox Width="40"
                             Text="{Binding S8TempHotSetErr}"
                              />
                </StackPanel>
            </Border>
            <Border Grid.Row="16" Grid.Column="5">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                    <TextBox Width="40"
                             Text="{Binding S8TempHotSet2}"
                              />
                    <TextBlock Text=" ± " />
                    <TextBox Width="40"
