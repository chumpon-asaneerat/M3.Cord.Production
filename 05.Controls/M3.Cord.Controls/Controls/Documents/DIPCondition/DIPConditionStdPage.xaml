<UserControl x:Class="M3.Cord.Controls.Documents.DIPConditionStdPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             d:DesignHeight="1660"
             d:DesignWidth="1000"
             mc:Ignorable="d">
    <UserControl.Resources>
        <!--  LOCAL STYLES  -->
        <Style TargetType="Border">
            <Setter Property="Padding" Value="5" />
            <Setter Property="BorderBrush" Value="DimGray" />
            <Setter Property="BorderThickness" Value="0.5" />
        </Style>
        <Style TargetType="TextBlock">
            <Setter Property="FontSize" Value="18" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="HorizontalAlignment" Value="Center" />
        </Style>
        <Style TargetType="TextBox" BasedOn="{StaticResource CommonTextBox}">
            <Setter Property="FontSize" Value="18" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="HorizontalAlignment" Value="Center" />
        </Style>
        <Style TargetType="CheckBox" BasedOn="{StaticResource CommonCheckBox}">
            <Setter Property="FontSize" Value="18" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="HorizontalAlignment" Value="Center" />
        </Style>
    </UserControl.Resources>
    <ScrollViewer>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
                <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <!--#region COLUMN HEADERS-->
            <Border Grid.Row="0"
                    Grid.Column="0"
                    Grid.ColumnSpan="3"
                    Background="Silver">
                <!--  CHECKING ITEM (MERGE COLUMN)  -->
                <TextBlock Text="รายการตรวจสอบ" />
            </Border>
            <Border Grid.Row="1"
                    Grid.Column="0"
                    Grid.ColumnSpan="3"
                    Background="Silver">
                <!--  MC Setting  -->
                <TextBlock Text="(Checking Item)" />
            </Border>
            <Border Grid.Row="0"
                    Grid.Column="3"
                    Grid.RowSpan="2"
                    Background="Silver">
                <!--  Class  -->
                <TextBlock HorizontalAlignment="Center">
                    <Run Text="Special" />
                    <LineBreak />
                    <Run Text="Char." />
                    <LineBreak />
                    <Run Text="Class" />
                </TextBlock>
            </Border>
            <Border Grid.Row="0"
                    Grid.Column="4"
                    Background="Silver">
                <!--  Class  -->
                <TextBlock HorizontalAlignment="Center">
                    <Run Text="   ค่าที่กำหนด" />
                    <LineBreak />
                    <Run Text="(Specification)" />
                </TextBlock>
            </Border>
            <Border Grid.Row="1"
                    Grid.Column="4"
                    Background="Silver">
                <!--  Class  -->
                <TextBlock Text="Set" />
            </Border>
            <Border Grid.Row="0"
                    Grid.Column="5"
                    Background="Silver">
                <!--  Class  -->
                <TextBlock HorizontalAlignment="Center">
                    <Run Text="   ค่าที่กำหนด" />
                    <LineBreak />
                    <Run Text="(Specification)" />
                </TextBlock>
            </Border>
            <Border Grid.Row="1"
                    Grid.Column="5"
                    Background="Silver">
                <!--  Class  -->
                <TextBlock Text="Set" />
            </Border>
            <!--#endregion-->

            <!--# 1.Yarn 1-->
            <Border Grid.Row="2" Grid.Column="0" Grid.RowSpan="9">
                <TextBlock Text="CS 7 Creel" HorizontalAlignment="Center" />
            </Border>
            <Border Grid.Row="2" Grid.Column="1" Grid.RowSpan="6">
                <TextBlock Text="1. Yarn" HorizontalAlignment="Center" />
            </Border>
            <Border Grid.Row="2" Grid.Column="2">
                <TextBlock HorizontalAlignment="Left">
                    <Run Text="1.1 Cord structure (โครงสร้างเชือก)" />
                </TextBlock>
            </Border>
            <Border Grid.Row="2" Grid.Column="3">
                <CheckBox Content="SC" IsChecked="{Binding S7YarnCordStructureSC}" />
            </Border>
            <Border Grid.Row="2" Grid.Column="4">
                <TextBox Width="100"
                             Text="{Binding S7YarnCordStructureSet}"
                              />
            </Border>
            <Border Grid.Row="2" Grid.Column="5">
                <TextBox Width="100"
                             Text="{Binding S7YarnCordStructureSet2}"
                              />
            </Border>
            <!--#endregion-->

            <!--# 1.Yarn 2-->           
            <Border Grid.Row="3" Grid.Column="2">
                <TextBlock HorizontalAlignment="Left">
                    <Run Text="1.2 Yarn type (ชนิดด้าย)" />
                </TextBlock>
            </Border>
            <Border Grid.Row="3" Grid.Column="3">
                <CheckBox Content="SC" IsChecked="{Binding S7YarnYarnTypeSC}" />
            </Border>
            <Border Grid.Row="3" Grid.Column="4">
                <TextBox Width="100"
                             Text="{Binding S7YarnYarnTypeSet}"
                              />
            </Border>
            <Border Grid.Row="3" Grid.Column="5">
                <TextBox Width="100"
                             Text="{Binding S7YarnYarnTypeSet2}"
                              />
            </Border>
            <!--#endregion-->

            <!--# 1.Yarn 3-->
            <Border Grid.Row="4" Grid.Column="2">
                <TextBlock HorizontalAlignment="Left">
                    <Run Text="1.3 1st twist (การตีเกลียวครั้งที่ 1)" />
                </TextBlock>
            </Border>
            <Border Grid.Row="4" Grid.Column="3">
                <CheckBox Content="SC" IsChecked="{Binding S7Yarn1stTwistSC}" />
            </Border>
            <Border Grid.Row="4" Grid.Column="4">
                <TextBox Width="100"
                             Text="{Binding S7Yarn1stTwistSet}"
                              />
            </Border>
            <Border Grid.Row="4" Grid.Column="5">
                <TextBox Width="100"
                             Text="{Binding S7Yarn1stTwistSet2}"
                              />
            </Border>
            <!--#endregion-->

            <!--# 1.Yarn 4-->
            <Border Grid.Row="5" Grid.Column="2">
                <TextBlock HorizontalAlignment="Left">
                    <Run Text="1.4 2nd twist (การตีเกลียวครั้งที่ 2)" />
                </TextBlock>
            </Border>
            <Border Grid.Row="5" Grid.Column="3">
                <CheckBox Content="SC" IsChecked="{Binding S7Yarn2ndTwistSC}" />
            </Border>
            <Border Grid.Row="5" Grid.Column="4">
                <TextBox Width="100"
                             Text="{Binding S7Yarn2ndTwistSet}"
                              />
            </Border>
            <Border Grid.Row="5" Grid.Column="5">
                <TextBox Width="100"
                             Text="{Binding S7Yarn2ndTwistSet2}"
                              />
            </Border>
            <!--#endregion-->

            <!--# 1.Yarn 5-->
