            <!--# 6. Speed -->
            <Border Grid.Row="19" Grid.Column="1" >
                <TextBlock Text="6. Speed" HorizontalAlignment="Center" />
            </Border>
            <Border Grid.Row="19" Grid.Column="2">
                <TextBlock HorizontalAlignment="Left">
                    <Run Text="6.1 Speed (m/min)(ความเร็วรอบ)" />
                </TextBlock>
            </Border>
            <Border Grid.Row="19" Grid.Column="3">
                <CheckBox Content="SC" IsChecked="{Binding S8SpeedSC}" />
            </Border>
            <Border Grid.Row="19" Grid.Column="4">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                    <TextBox Width="40"
                             Text="{Binding S8SpeedSet}"
