                <TextBlock HorizontalAlignment="Left">
                    <Run Text="1.5 Label of yarn(ลาเบลชี้บ่งบนลูกด้าย)" />
                </TextBlock>
            </Border>
            <Border Grid.Row="6" Grid.Column="3">
                <CheckBox Content="SC" IsChecked="{Binding S7YarnLabelOfYarnSC}" />
            </Border>
            <Border Grid.Row="6" Grid.Column="4">
                <TextBox Width="100"
                             Text="{Binding S7YarnLabelOfYarnSet}"
                              />
            </Border>
            <Border Grid.Row="6" Grid.Column="5">
                <TextBox Width="100"
                             Text="{Binding S7YarnLabelOfYarnSet2}"
                              />
            </Border>
            <!--#endregion-->

            <!--# 1.Yarn 6-->
            <Border Grid.Row="7" Grid.Column="2">
                <TextBlock HorizontalAlignment="Left">
                    <Run Text="1.6 Weight (Kg) น้ำหนัก (กก.)" />
                </TextBlock>
            </Border>
            <Border Grid.Row="7" Grid.Column="3">
                <CheckBox Content="SC" IsChecked="{Binding S7YarnWeightSC}" />
            </Border>
            <Border Grid.Row="7" Grid.Column="4">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                    <TextBox Width="40"
                             Text="{Binding S7YarnWeightSet}"
                              />
                    <TextBlock Text=" ± " />
                    <TextBox Width="40"
                             Text="{Binding S7YarnWeightSetErr}"
                              />
                </StackPanel>
            </Border>
            <Border Grid.Row="7" Grid.Column="5">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                    <TextBox Width="40"
                             Text="{Binding S7YarnWeightSet2}"
                              />
                    <TextBlock Text=" ± " />
                    <TextBox Width="40"
                             Text="{Binding S7YarnWeightSetErr2}"
                              />
                </StackPanel>
            </Border>
            <!--#endregion-->

            <!--# 2.Creel setting 1-->
            <Border Grid.Row="8" Grid.Column="1" Grid.RowSpan="3">
                <TextBlock Text="2. Creel setting" HorizontalAlignment="Center" />
            </Border>
            <Border Grid.Row="8" Grid.Column="2">
                <TextBlock HorizontalAlignment="Left">
                    <Run Text="2.1 Use tensor (ให้น้ำหนักทับ)" />
                </TextBlock>
            </Border>
            <Border Grid.Row="8" Grid.Column="3">
                <CheckBox Content="SC" IsChecked="{Binding S7CreelSettingUseTensorSC}" />
            </Border>
            <Border Grid.Row="8" Grid.Column="4">
                <TextBox Width="100"
                             Text="{Binding S7CreelSettingUseTensorSet}"
                              />
            </Border>
            <Border Grid.Row="8" Grid.Column="5">
                <TextBox Width="100"
                             Text="{Binding S7CreelSettingUseTensorSet2}"
                              />
            </Border>
            <!--#endregion-->

            <!--# 2.Creel setting 2-->
            <Border Grid.Row="9" Grid.Column="2">
                <TextBlock HorizontalAlignment="Left">
                    <Run Text="2.2 Knot Condition (สภาวะการต่อด้าย)" />
                </TextBlock>
            </Border>
            <Border Grid.Row="9" Grid.Column="3">
                <CheckBox Content="SC" IsChecked="{Binding S7CreelSettingKnotConditionSC}" />
            </Border>
            <Border Grid.Row="9" Grid.Column="4">
                <TextBox Width="100"
                             Text="{Binding S7CreelSettingKnotConditionSet}"
                              />
            </Border>
            <Border Grid.Row="9" Grid.Column="5">
                <TextBox Width="100"
                             Text="{Binding S7CreelSettingKnotConditionSet2}"
                              />
            </Border>
            <!--#endregion-->

            <!--# 2.Yarn 3-->
            <Border Grid.Row="10" Grid.Column="2">
                <TextBlock HorizontalAlignment="Left">
                    <Run Text="2.3 ตัวดักปม ( Slub catswe )" />
                </TextBlock>
            </Border>
            <Border Grid.Row="10" Grid.Column="3">
                <CheckBox Content="SC" IsChecked="{Binding S7CreelSettingSlubCatsweSC}" />
            </Border>
            <Border Grid.Row="10" Grid.Column="4">
                <TextBox Width="100"
                             Text="{Binding S7CreelSettingSlubCatsweSet}"
                              />
            </Border>
            <Border Grid.Row="10" Grid.Column="5">
                <TextBox Width="100"
                             Text="{Binding S7CreelSettingSlubCatsweSet2}"
                              />
            </Border>
            <!--#endregion-->

            <!--# 3.Stretch 1-->
            <Border Grid.Row="11" Grid.Column="0" Grid.RowSpan="20">
                <TextBlock Text="CS-8 Cordseter " HorizontalAlignment="Center" />
            </Border>
            <Border Grid.Row="11" Grid.Column="1" Grid.RowSpan="4">
                <TextBlock Text="3. Stretch" HorizontalAlignment="Center" />
            </Border>
            <Border Grid.Row="11" Grid.Column="2">
                <TextBlock HorizontalAlignment="Left">
                    <Run Text="3.1 Stretch  Dryer (%การยืด ช่วงทำให้แห้ง)" />
                </TextBlock>
            </Border>
            <Border Grid.Row="11" Grid.Column="3">
                <CheckBox Content="SC" IsChecked="{Binding S8StretchDryerSC}" />
            </Border>
            <Border Grid.Row="11" Grid.Column="4">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                    <TextBox Width="40"
                             Text="{Binding S8StretchDryerSet}"
                              />
                    <TextBlock Text=" ± " />
                    <TextBox Width="40"
                             Text="{Binding S8StretchDryerSetErr}"
                              />
                </StackPanel>
            </Border>
            <Border Grid.Row="11" Grid.Column="5">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                    <TextBox Width="40"
                             Text="{Binding S8StretchDryerSet2}"
                              />
                    <TextBlock Text=" ± " />
                    <TextBox Width="40"
                             Text="{Binding S8StretchDryerSetErr2}"
                              />
                </StackPanel>
            </Border>
            <!--#endregion-->

            <!--# 3.Stretch 2-->
            <Border Grid.Row="12" Grid.Column="2">
                <TextBlock HorizontalAlignment="Left">
                    <Run Text="3.2 Stretch Hot ( %การยืด ช่วงร้อน)" />
                </TextBlock>
            </Border>
            <Border Grid.Row="12" Grid.Column="3">
                <CheckBox Content="SC" IsChecked="{Binding S8StretchStretchHotSC}" />
            </Border>
            <Border Grid.Row="12" Grid.Column="4">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                    <TextBox Width="40"
                             Text="{Binding S8StretchStretchHotSet}"
                              />
                    <TextBlock Text=" ± " />
                    <TextBox Width="40"
                             Text="{Binding S8StretchStretchHotSetErr}"
                              />
                </StackPanel>
            </Border>
            <Border Grid.Row="12" Grid.Column="5">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                    <TextBox Width="40"
                             Text="{Binding S8StretchStretchHotSet2}"
                              />
                    <TextBlock Text=" ± " />
                    <TextBox Width="40"
                             Text="{Binding S8StretchStretchHotSetErr2}"
                              />
                </StackPanel>
            </Border>
            <!--#endregion-->

            <!--# 3.Stretch 3-->
            <Border Grid.Row="13" Grid.Column="2">
                <TextBlock HorizontalAlignment="Left">
                    <Run Text="3.3 Stretch Normal( %การยืด ช่วงปกติ)" />
                </TextBlock>
            </Border>
            <Border Grid.Row="13" Grid.Column="3">
                <CheckBox Content="SC" IsChecked="{Binding S8StretchNormalSC}" />
            </Border>
            <Border Grid.Row="13" Grid.Column="4">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                    <TextBox Width="40"
                             Text="{Binding S8StretchNormalSet}"
                              />
                    <TextBlock Text=" ± " />
                    <TextBox Width="40"
                             Text="{Binding S8StretchNormalSetErr}"
                              />
                </StackPanel>
            </Border>
            <Border Grid.Row="13" Grid.Column="5">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                    <TextBox Width="40"
                             Text="{Binding S8StretchNormalSet2}"
                              />
                    <TextBlock Text=" ± " />
                    <TextBox Width="40"
                             Text="{Binding S8StretchNormalSetErr2}"
                              />
                </StackPanel>
            </Border>
            <!--#endregion-->

            <!--# 3.Stretch 4-->
            <Border Grid.Row="14" Grid.Column="2">
                <TextBlock HorizontalAlignment="Left">
                    <Run Text="3.4 Stretch Total ( %การยืด รวม)" />
                </TextBlock>
            </Border>
            <Border Grid.Row="14" Grid.Column="3">
                <CheckBox Content="SC" IsChecked="{Binding S8StretchTotalSC}" />
            </Border>
            <Border Grid.Row="14" Grid.Column="4">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                    <TextBox Width="40"
                             Text="{Binding S8StretchTotalSet}"
                              />
                    <TextBlock Text=" ± " />
                    <TextBox Width="40"
                             Text="{Binding S8StretchTotalSetErr}"
                              />
                </StackPanel>
            </Border>
            <Border Grid.Row="14" Grid.Column="5">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                    <TextBox Width="40"
                             Text="{Binding S8StretchTotalSet2}"
                              />
                    <TextBlock Text=" ± " />
                    <TextBox Width="40"
                             Text="{Binding S8StretchTotalSetErr2}"
                              />
                </StackPanel>
            </Border>
