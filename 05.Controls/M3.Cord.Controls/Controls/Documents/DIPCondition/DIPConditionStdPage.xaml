            <!--# 4.Temp 1-->
            <Border Grid.Row="15" Grid.Column="1" Grid.RowSpan="3">
                <TextBlock Text="4.Temp" HorizontalAlignment="Center" />
            </Border>
            <Border Grid.Row="15" Grid.Column="2">
                <TextBlock HorizontalAlignment="Left">
                    <Run Text="4.1 Temp  Dryer (อุณหภูมิ ช่วงทำให้แห้ง)" />
