            <!--# 4.Temp 2-->
            <Border Grid.Row="16" Grid.Column="2">
                <TextBlock HorizontalAlignment="Left">
                    <Run Text="4.2 Temp  Hot (อุณหภูมิ ช่วงร้อน)" />
                </TextBlock>
            </Border>
            <Border Grid.Row="16" Grid.Column="3">
                <CheckBox Content="SC" IsChecked="{Binding S8TempHotSC}" />
            </Border>
            <Border Grid.Row="16" Grid.Column="4">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                    <TextBox Width="40"
