            <Border Grid.Row="17" Grid.Column="5">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                    <TextBox Width="40"
                             Text="{Binding S8TempNormalSet2}"
                              />
                    <TextBlock Text=" ± " />
                    <TextBox Width="40"
                             Text="{Binding S8TempNormalSetErr2}"
                              />
                </StackPanel>
            </Border>
