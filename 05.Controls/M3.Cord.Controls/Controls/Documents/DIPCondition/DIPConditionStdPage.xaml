            <!--# 7. No. of cord -->
            <Border Grid.Row="20" Grid.Column="1" >
                <TextBlock Text="7. No. of cord" HorizontalAlignment="Center" />
            </Border>
            <Border Grid.Row="20" Grid.Column="2">
