            <!--# 5. Counter -->
            <Border Grid.Row="18" Grid.Column="1" >
                <TextBlock Text="5. Counter" HorizontalAlignment="Center" />
            </Border>
            <Border Grid.Row="18" Grid.Column="2">
                <TextBlock HorizontalAlignment="Left">
                    <Run Text="5.1 Counter setting (m) (การตั้งความยาว)" />
                </TextBlock>
            </Border>
            <Border Grid.Row="18" Grid.Column="3">
                <CheckBox Content="SC" IsChecked="{Binding S8CounterSettingSC}" />
            </Border>
            <Border Grid.Row="18" Grid.Column="4">
