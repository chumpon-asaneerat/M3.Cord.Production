                            </Style.Triggers>
                        </Style>
                    </TextBlock.Style>
                </TextBlock>
            </Border>
            <Border Grid.Row="42" Grid.Column="4">
                <TextBox Width="220"
                         Text="{Binding S9SpongSet}"
                         IsReadOnly="True" />
            </Border>
            <Border Grid.Row="42" Grid.Column="5">
                <TextBox Width="220" Text="{Binding S9SpongActual}" />
            </Border>
            <Border Grid.Row="42" Grid.Column="6">
                <TextBox Width="220"
                         Text="{Binding S9SpongSet2}"
                         IsReadOnly="True" />
            </Border>
            <Border Grid.Row="42" Grid.Column="7">
                <TextBox Width="220" Text="{Binding S9SpongActual2}" />
            </Border>
            <!--#endregion-->

        </Grid>
    </ScrollViewer>
</UserControl>

