<Window x:Class="M3.Cord.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Title="M3 Cord Developer Tools"
        Height="450"
        Width="800"
        Loaded="Window_Loaded"
        Closing="Window_Closing"
        mc:Ignorable="d">
    <DockPanel Margin="5">
        <StackPanel Orientation="Vertical" DockPanel.Dock="Left" Width="200" Margin="2">
            <Button x:Name="cmdImportItemCode" Content="Import Item Code" Click="cmdImportItemCode_Click" />
        </StackPanel>
        <Grid />
    </DockPanel>
</Window>
