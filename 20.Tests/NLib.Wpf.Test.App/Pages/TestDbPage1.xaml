<UserControl x:Class="NLib.Wpf.Test.App.Pages.TestDbPage1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:page="clr-namespace:NLib.Wpf.Pages;assembly=NLib.Wpf.Controls"
             xmlns:ctrl="clr-namespace:NLib.Wpf.Controls;assembly=NLib.Wpf.Controls"
             mc:Ignorable="d"
             d:DesignHeight="450"
             d:DesignWidth="800"
             Loaded="UserControl_Loaded"
             Unloaded="UserControl_Unloaded">
    <page:NPage HeaderText="Test DB 1">
        <DockPanel LastChildFill="True">
            <StackPanel Orientation="Horizontal" DockPanel.Dock="Top" 
                        Margin="5" 
                        Height="40">
                <Button x:Name="cmdGets" Content="Gets" Margin="2,0" Width="150" />
                <Button x:Name="cmdSave" Content="Save" Margin="2,0" Width="150" />
            </StackPanel>
            <DataGrid x:Name="grid" Margin="5" AutoGenerateColumns="True" />
        </DockPanel>
    </page:NPage>
</UserControl>
